# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
*/db/*.sqlite3
*/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
*/log/*
*/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development.
*/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore Yarn files
/yarn-error.log
/node_modules

# Ignore precompiled assets.
/public/assets

# Ignore the assets directory.
/app/assets/*

# Ignore application-specific files
/public/packs
/public/packs-test

# Ignore coverage report generated by simplecov.
coverage/

# Ignore node modules
/node_modules

# Ignore secret files.
/.env
.env.*
.env.local
.env.*.local

# Ignore by-products of bundler
/vendor/bundle

# Ignore system specific files
.DS_Store
Thumbs.db

# Ignore IDE and editor specific files
.idea/
*.swp
*.swo
.vscode/
